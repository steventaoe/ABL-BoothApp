{
  "$schema": "../gen/schemas/mobile-schema.json",
  "identifier": "mobile",
  "description": "Capability for the mobile window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:event:default",
    "opener:default",
    "fs:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "http://localhost:5140/**"
        },
        {
          "url": "http://127.0.0.1:5140/**"
        },
        {
          "url": "http://**"
        },
        {
          "url": "https://**"
        }
      ]
    }
  ]
}